-- Toggle para Auto Farm com Blobman
BlobmanTab:AddToggle({
    Name = "Auto Farm com Blobman",
    Default = false,
    Callback = function(Value)
        blobmanAutoFarm = Value
        if Value then
            OrionLib:MakeNotification({
                Name = "Auto Farm Ativado",
                Content = "Auto Farm com Blobman ativado",
                Time = 3
            })
            -- Inicia o auto farm
            coroutine.wrap(blobmanFarmF)()
        else
            OrionLib:MakeNotification({
                Name = "Auto Farm Desativado",
                Content = "Auto Farm com Blobman desativado",
                Time = 3
            })
        end
    end
