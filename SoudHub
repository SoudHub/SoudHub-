-- Slider para velocidade do farm
BlobmanTab:AddSlider({
    Name = "Velocidade do Farm",
    Min = 0.5,
    Max = 5,
    Default = 1,
    Color = Color3.fromRGB(255,255,255),
    Increment = 0.5,
    ValueName = "segundos",
    Callback = function(Value)
        blobmanFarmSpeed = Value
    end
})

-- Botão para fazer Blobman te seguir
BlobmanTab:AddButton({
    Name = "Fazer Blobman me Seguir",
    Callback = function()
        if blobmanInstanceS then
            local char = plr.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                local humanoid = blobmanInstanceS:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid:MoveTo(char.HumanoidRootPart.Position)
                    OrionLib:MakeNotification({
                        Name = "Blobman Seguindo",
                        Content = "Blobman está seguindo você",
                        Time = 3
                    })
                end
            end
        else
            OrionLib:MakeNotification({
                Name = "Erro",
                Content = "Nenhum Blobman encontrado",
                Time = 3
            })
        end
    end
})

-- Botão para Auto Gucci (também na aba Blobman)
BlobmanTab:AddButton({
    Name = "Auto Gucci ▶",
    Callback = function()
        autoGucciT = true
        spawnBlobmanF()
        task.wait(1.1)
        if not sitJumpT then
            coroutine.wrap(sitJumpF)()
            sitJumpT = true
        end
        coroutine.wrap(ragdollLoopF)()
        
        OrionLib:MakeNotification({
            Name = "Auto Gucci Ativado!",
            Content = "Executando Auto Gucci por 6 segundos...",
            Time = 3
        })
    end
})

-- Toggle para destruir automaticamente (também na aba Blobman)
BlobmanTab:AddToggle({
    Name = "Destruir Auto Gucci",
    Default = true,
    Callback = function(Value)
        destroyAutoGucciT = Value
    end
})

